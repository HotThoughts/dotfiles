[
  {
    "bindings": {
      "cmd-o": "file_finder::Toggle",
      "cmd-p": "command_palette::Toggle",
      "cmd-\\": "workspace::ToggleLeftDock",
      "cmd-|": "workspace::ToggleRightDock",
      "ctrl-/": "workspace::ToggleBottomDock",
      "ctrl-.": "agent::ToggleFocus",
    },
  },
  {
    "context": "Workspace",
    "bindings": {
      "cmd-shift-m": "workspace::ToggleZoom",
    },
  },
  {
    "context": "Editor",
    "bindings": {},
  },
  {
    "context": "Dock",
    "bindings": {
      "ctrl-w h": "workspace::ActivatePaneLeft",
      "ctrl-w l": "workspace::ActivatePaneRight",
      "ctrl-w k": "workspace::ActivatePaneUp",
      "ctrl-w j": "workspace::ActivatePaneDown",
    },
  },
  {
    "context": "vim_mode == visual",
    "bindings": {
      "shift-s": ["vim::PushAddSurrounds", {}],
    },
  },
  {
    "context": "vim_mode == normal || vim_mode == visual",
    "bindings": {
      "s": "vim::PushSneak",
      "shift-s": "vim::PushSneakBackward",
    },
  },
  {
    "context": "VimControl && !menu && vim_mode != operator",
    "bindings": {
      "w": "vim::NextSubwordStart",
      "b": "vim::PreviousSubwordStart",
      "e": "vim::NextSubwordEnd",
      "g e": "vim::PreviousSubwordEnd",
    },
  },
  {
    "context": "Editor && (vim_mode == normal || vim_mode == visual) && !VimWaiting && !menu",
    "bindings": {
      "space": null,
      "space e": "project_panel::ToggleFocus",
      "space f b": "tab_switcher::Toggle",
      "space f g": "workspace::NewSearch",
      "space f s": "project_symbols::Toggle",
      "space space": "file_finder::Toggle",
      "space t": "workspace::NewCenterTerminal",
      "space d d": "diagnostics::Deploy",
      "space d w": "diagnostics::ToggleWarnings",
      "space d o": "editor::GoToDiagnostic",
      "space s": "outline::Toggle",
      "space a": "agent::ToggleFocus",
      "space z": "workspace::ToggleZoom",
      "space m": "markdown::OpenPreview",
      "space g g": "git_panel::ToggleFocus",
      "space g c": "git::GenerateCommitMessage",
      "space g b": "editor::OpenGitBlameCommit",
      "space r": "task::Spawn",
      "space j j": [
        "task::Spawn",
        { "task_name": "JJUI", "reveal_target": "center" },
      ],
      "space f f": [
        "task::Spawn",
        { "task_name": "Television File Finder", "reveal_target": "center" },
      ],
    },
  },
  {
    "context": "vim_mode == normal && !menu",
    "bindings": {
      "shift-y": ["workspace::SendKeystrokes", "y $"],
      "'": "vim::ToggleMarksView",
    },
  },
]
